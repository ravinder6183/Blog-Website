<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="input1">Title</label>
        <input class="form-control" type="text" name="input1">
    </div>
    
    
    <div class="form-group">
        <label for="textArea">Post</label>
        <textarea class="form-control" name="textArea" id="" cols="30" rows="5"></textarea>
    </div>
    <div>
        <button type="submit" name="button" class="btn btn-primary">Publish</button>
    </div>
    
</form>
